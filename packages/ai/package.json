{
  "name": "@nwrx/ai",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Nanoworks <dev@nwrx.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./chat": {
      "types": "./dist/chat.d.ts",
      "import": "./dist/chat.js",
      "require": "./dist/chat.cjs"
    },
    "./embeddings": {
      "types": "./dist/embeddings.d.ts",
      "import": "./dist/embeddings.js",
      "require": "./dist/embeddings.cjs"
    },
    "./image": {
      "types": "./dist/image.d.ts",
      "import": "./dist/image.js",
      "require": "./dist/image.cjs"
    },
    "./providers": {
      "types": "./dist/providers.d.ts",
      "import": "./dist/providers.js",
      "require": "./dist/providers.cjs"
    },
    "./models": {
      "types": "./dist/models.d.ts",
      "import": "./dist/models.js",
      "require": "./dist/models.cjs"
    },
    "./speech": {
      "types": "./dist/speech.d.ts",
      "import": "./dist/speech.js",
      "require": "./dist/speech.cjs"
    },
    "./transcript": {
      "types": "./dist/transcript.d.ts",
      "import": "./dist/transcript.js",
      "require": "./dist/transcript.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@unshared/functions": "0.6.6",
    "@unshared/string": "0.6.6",
    "@unshared/types": "0.6.6",
    "jsonschema": "1.5.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.57.0",
    "msw": "2.7.3",
    "openai": "5.8.2",
    "tsup": "8.5.0"
  }
}
