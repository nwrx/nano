{
  "name": "@nwrx/nano-module-core",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/module-core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./index.ts",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./types/index.ts",
      "require": "./dist/types/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "tsup ./index.ts --format cjs,esm --watch",
    "build": "tsup ./index.ts --format cjs,esm --clean"
  },
  "dependencies": {
    "@nwrx/nano": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@unshared/client": "0.4.1",
    "@unshared/fs": "0.4.1",
    "@unshared/string": "0.4.1",
    "@unshared/types": "0.4.1",
    "@unshared/validation": "0.4.1",
    "openapi-types": "12.1.3",
    "postman-to-openapi": "3.0.1",
    "yaml": "2.7.0"
  },
  "tsup": {
    "entry": [
      "index.ts",
      "types/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "treeshake": true,
    "sourcemap": true,
    "splitting": true,
    "dts": true
  }
}
