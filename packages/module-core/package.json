{
  "name": "@nwrx/module-core",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/module-core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./categories": {
      "types": "./dist/categories/index.d.ts",
      "import": "./dist/categories/index.js",
      "require": "./dist/categories/index.cjs"
    },
    "./nodes": {
      "types": "./dist/nodes/index.d.ts",
      "import": "./dist/nodes/index.js",
      "require": "./dist/nodes/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@nwrx/core": "workspace:*",
    "@types/json-schema": "7.0.15",
    "@unshared/string": "0.3.1",
    "@unshared/types": "0.3.1",
    "@unshared/validation": "0.3.1",
    "yaml": "2.6.1"
  }
}
