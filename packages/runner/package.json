{
  "name": "@nwrx/nano-runner",
  "type": "module",
  "private": true,
  "bin": "./dist/server.mjs",
  "exports": {
    ".": {
      "types": "./index.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess 'node dist/server.mjs'",
    "build": "tsup",
    "start": "node dist/server.mjs"
  },
  "dependencies": {
    "@nwrx/nano": "workspace:*",
    "@unserved/client": "0.3.3",
    "@unserved/server": "0.3.3",
    "@unshared/functions": "0.6.0",
    "@unshared/process": "0.4.1",
    "@unshared/string": "0.6.0",
    "@unshared/types": "0.6.0",
    "@unshared/validation": "0.6.0",
    "consola": "3.4.1",
    "crossws": "0.3.4",
    "h3": "1.15.1",
    "isolated-vm": "5.0.4",
    "typeorm": "0.3.21"
  },
  "devDependencies": {
    "@types/ws": "8.18.0",
    "tsup": "8.4.0",
    "ws": "8.18.1"
  }
}
