{
  "name": "@nwrx/core",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Nanoworks <dev@nwrx.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.cjs",
      "import": "./dist/components/index.js"
    },
    "./sandbox": {
      "types": "./dist/sandbox/index.d.ts",
      "require": "./dist/sandbox/index.cjs",
      "import": "./dist/sandbox/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.cjs",
      "import": "./dist/utils/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=16384' tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@unshared/client": "0.4.0",
    "@unshared/collection": "0.4.0",
    "@unshared/decorators": "0.4.0",
    "@unshared/functions": "0.4.0",
    "@unshared/string": "0.4.0",
    "@unshared/types": "0.4.0",
    "@unshared/validation": "0.4.0",
    "esbuild": "0.24.2",
    "isolated-vm": "5.0.3",
    "openapi-types": "12.1.3",
    "yaml": "2.6.1"
  },
  "devDependencies": {
    "consola": "3.3.3",
    "msw": "2.7.0"
  },
  "tsup": {
    "entry": {
      "components": "components/index.ts",
      "sandbox": "sandbox/index.ts",
      "utils": "utils/index.ts",
      "index": "index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "target": "node20",
    "dts": true,
    "shims": true,
    "clean": true,
    "splitting": true,
    "sourcemap": true,
    "treeshaking": true,
    "experimental-dts": true
  }
}
