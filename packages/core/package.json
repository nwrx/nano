{
  "name": "@nwrx/core",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./thread": {
      "types": "./dist/thread/index.d.ts",
      "import": "./dist/thread/index.js",
      "require": "./dist/thread/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=16384' tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@unshared/client": "0.3.2",
    "@unshared/collection": "0.3.2",
    "@unshared/decorators": "0.3.2",
    "@unshared/string": "0.3.2",
    "@unshared/types": "0.3.2",
    "@unshared/validation": "0.3.2",
    "openapi-types": "12.1.3",
    "yaml": "2.6.1"
  }
}
