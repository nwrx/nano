{
  "name": "@nwrx/nano",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./thread": {
      "types": "./dist/thread/index.d.ts",
      "import": "./dist/thread/index.js",
      "require": "./dist/thread/index.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@unshared/collection": "0.4.1",
    "@unshared/decorators": "0.4.1",
    "@unshared/string": "0.4.1",
    "@unshared/types": "0.4.1",
    "@unshared/validation": "0.4.1",
    "openapi-types": "12.1.3",
    "yaml": "2.6.1"
  },
  "tsup": {
    "entry": [
      "index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "treeshake": true,
    "sourcemap": true,
    "dts": true
  }
}
