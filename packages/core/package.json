{
  "name": "@nwrx/nano",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "author": "Nanoworks <dev@nwrx.io>",
  "bugs": "https://github.com/nwrx/nano-ce/issues",
  "homepage": "https://github.com/nwrx/nano-ce#readme",
  "repository": {
    "directory": "packages/core",
    "type": "git",
    "url": "git@github.com:nwrx/nano-ce.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./sandbox": {
      "types": "./dist/sandbox.d.ts",
      "require": "./dist/sandbox.cjs",
      "import": "./dist/sandbox.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.12.1",
    "@types/json-schema": "7.0.15",
    "@unshared/client": "0.6.5",
    "@unshared/collection": "0.6.5",
    "@unshared/decorators": "0.6.5",
    "@unshared/functions": "0.6.5",
    "@unshared/string": "0.6.5",
    "@unshared/types": "0.6.5",
    "@unshared/validation": "0.6.5",
    "esbuild": "0.25.1",
    "isolated-vm": "5.0.4",
    "openapi-types": "12.1.3",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "0.56.0",
    "consola": "3.4.1",
    "h3": "1.15.3",
    "jsonschema": "1.5.0",
    "msw": "2.7.3",
    "openai": "5.8.2",
    "zod": "3.24.2",
    "zod-to-json-schema": "3.24.5"
  },
  "tsup": {
    "entry": {
      "components": "components/index.ts",
      "sandbox": "sandbox/index.ts",
      "utils": "utils/index.ts",
      "index": "index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "target": "node20",
    "dts": true,
    "shims": true,
    "splitting": false,
    "sourcemap": true,
    "treeshaking": false,
    "experimental-dts": true
  }
}
